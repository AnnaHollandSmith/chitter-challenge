<h2>Peep Feed</h2>

<ul id='peeps'>
  <% @peeps.reverse.each do |peep| %>
    <li>
      Peep: <%= peep.text %>
      Posted on:   <%= peep.time.strftime('%m/%d/%y at %I:%M%p')%>
    </li>
  <% end %>
</ul>


<% if current_user == nil %>
  <form action='/sessions/new'>
    <input type='submit' value='Sign in'>
  </form>
  <form action='users/new'>
    <input type='submit' value='Sign up'>
  </form>
<% end %>
